{
  "name": "aws-architecture-agent",
  "version": "1.0.0",
  "description": "AWS Architecture Agent - A tool for analyzing, validating, and generating AWS architecture diagrams",
  "main": "dist/src/cli/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/cli/main.js",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node src/cli/main.ts",
    "demo": "ts-node demo/nlp-demo.ts",
    "demo-input": "ts-node demo/input-parser-demo.ts",
    "demo-docs": "ts-node demo/documentation-demo.ts",
    "test": "vitest",
    "test:unit": "vitest",
    "test:integration": "vitest test/integration/",
    "test:validation": "vitest test/validation/",
    "test:performance": "vitest test/performance/",
    "test:error": "vitest test/error-handling/",
    "test:coverage": "vitest --config=test/coverage/vitest-coverage.config.js",
    "test:quality": "node test/coverage/test-quality-metrics.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:validation && npm run test:performance && npm run test:error",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "aws",
    "architecture",
    "diagrams",
    "validation",
    "best-practices"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "ajv": "^8.17.1",
    "commander": "^11.0.0",
    "js-yaml": "^4.1.1",
    "mermaid": "^10.0.0",
    "pdf-lib": "^1.17.1",
    "ts-node": "^10.0.0",
    "uuid": "^8.3.2"
  }
}
